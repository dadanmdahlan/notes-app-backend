{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle" : "off",
        "linebreak-style" : "off",
        "no-unused-vars" : "off",
        "eol-last" : "off",
        "no-case-declarations" :"off",
        "camelcase": "off",
        "max-len":"off" 

    }
}
